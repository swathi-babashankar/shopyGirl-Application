{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "/api/index.js",
      "use":"@vercel/node"
    }
    
  ],
  
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
,

  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js"}
  ]
}
