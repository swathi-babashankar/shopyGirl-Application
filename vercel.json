{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "./api/index.js",
      "use":"@vercel/node"
    }
    
  ],
  
    "functions": {
  "api/**/*.js": {
    "maxDuration": 20,
    "memory": 2048
  }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.js"}
  ]
}
